<!DOCTYPE html>
<html>
  <head>
    <title>Photogram</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

  	<nav class="navbar navbar-default navbar-container navabar-header">
  		
  			
  <button class="navbar-toggle collapsed" type="button" data-toggle="collapse" data-target="#bs-navbar-collapse-1">
    <span class="sr-only"></span>
    <span class="icon-bar"></span>
    <span class="icon-bar"></span>
  </button>
  	<%=link_to "Photogram", root_path, :class=>"navbar-brand" %>

  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="nav navbar-nav navbar-right">
     
      <li class="nav-item">
        <%= link_to "New Post", new_post_path %>
      </li>
      <li class="nav-item">
        <%= link_to "Login", '#' %>
      </li>
      <li><%=link_to "Register", '#'%></li>
    </ul>

  </div>


</nav>


    <%= yield %>
