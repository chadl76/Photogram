
 <div class="row posts-wrapper">
<% @posts.each do |post| %>

  	<div class="post">
  	<div class="post-head">
  		<div class="name">UserName</div>
  	</div>
  		<div class="image center-block">
    	  <%= link_to (image_tag post.image.url, width: '640x', class: 'img-responsive'), post_path(post)%>
    	</div>
    	<p class="caption">
    	 <%= post.caption %>
    	</p>
  		


<%end%>
</div>