
<% if current_user liked_post(post) %>
$("#like_<%=@post.id%>").removeClass('glyphicon glyphicon-heart-empty').addClass('glyphicon glyphicon-heart heart');

<%else%>
$("#likes_<%=@post.id %>").html("<%= j (render partial: 'posts/likes', locals: { post: @post }) %>");

<%end %>
